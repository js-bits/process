{
  "name": "@js-bits/process",
  "version": "0.0.2",
  "description": "Asynchronous multi-step processing",
  "keywords": [
    "javascript",
    "asynchronous",
    "async",
    "processing",
    "process",
    "multi-step",
    "multistep",
    "steps",
    "sequence",
    "sequential"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./index.js"
    }
  },
  "scripts": {
    "build": "rollup ./index.js --format cjs --file dist/index.cjs --exports named",
    "lint": "eslint '**/*.{js,jsx}'",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-cjs": "jest --verbose",
    "test-coverage": "yarn test --coverage",
    "test-watch": "yarn test --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/process"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^0.3.1",
    "@types/jest": "^29.1.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "rollup": "^2.79.1"
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "testEnvironment": "node"
      },
      {
        "displayName": "jsdom",
        "testEnvironment": "jsdom"
      }
    ]
  }
}
